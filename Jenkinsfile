pipeline{
  agent any
  stages{
    stage('Clonar el codigo'){
      steps{
        echo 'Primero Job como codigo'
      }
    }
    stage('Compilar el codigo'){
      steps{
        echo 'Segundo Job como codigo'
      }
    }
    stage('Desplegar el codigo'){
      steps{
        echo 'Tercero Job como codigo'
      }
    }
  }
}
/
